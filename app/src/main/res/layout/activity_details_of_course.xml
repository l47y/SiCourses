<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DetailsOfCourseActivity">

    <TextView
            android:text="Nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsNombreLabel"
            android:gravity="right|center_horizontal"
            app:layout_constraintEnd_toStartOf="@+id/detailsNombre"
            android:layout_marginEnd="24dp" app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="32dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsNombre"
            android:text="TextView"
            android:gravity="center_horizontal"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="32dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="200dp"/>
    <TextView
            android:text="Empresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsEmpresaLabel"
            android:gravity="center_horizontal"
            app:layout_constraintEnd_toStartOf="@+id/detailsEmpresa"
            android:layout_marginEnd="24dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsNombreLabel"/>
    <TextView

            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsEmpresa"
            android:gravity="center_horizontal"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/detailsNombre"
            android:layout_marginEnd="200dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Lugar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsLugarLabel"
            android:gravity="center_horizontal"
            app:layout_constraintEnd_toStartOf="@+id/detailsLugar"
            android:layout_marginEnd="24dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsEmpresaLabel"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsLugar"
            android:gravity="center_horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="200dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsEmpresa"/>
    <TextView
            android:text="Fecha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsFechaLabel"
            android:gravity="center_horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsLugarLabel"
            app:layout_constraintEnd_toStartOf="@+id/detailsFecha"
            android:layout_marginEnd="24dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsFecha"
            android:gravity="center_horizontal"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/detailsLugar"
            android:layout_marginEnd="200dp" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="Participantes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsParticipantsLabelLabel"
            android:gravity="center_horizontal"
            app:layout_constraintEnd_toStartOf="@+id/detailsParticipants" android:layout_marginEnd="24dp"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/detailsFechaLabel"/>
    <TextView
            android:text="TextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/detailsParticipants"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="200dp"
            android:gravity="center_horizontal" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsFecha"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsEvaluacionesLabel"
            android:text="Evaluaciones"
            android:gravity="center_horizontal"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsParticipantsLabelLabel"
            app:layout_constraintEnd_toStartOf="@+id/detailsEvaluaciones" android:layout_marginEnd="24dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsEvaluaciones"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="200dp"
            android:gravity="center_horizontal" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsParticipants"/>
    <TextView
            android:text="Media"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsMediaLabel"
            android:gravity="center_horizontal"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/detailsEvaluacionesLabel"
            app:layout_constraintEnd_toStartOf="@+id/detailsMedia" android:layout_marginEnd="24dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/detailsMedia"
            android:gravity="center_horizontal" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/detailsEvaluaciones" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="200dp"/>
</android.support.constraint.ConstraintLayout>