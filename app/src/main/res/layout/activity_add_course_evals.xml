<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AddCourseEvals">

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_diez"
            android:layout_width="0dp"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 10s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginStart="16dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"/>

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_nueve"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 9s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toTopOf="parent"
    />

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_ocho"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 8s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginStart="16dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/croller_diez"/>

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_siete"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 7s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/croller_nueve"/>

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_seis"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 6s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/croller_ocho"
            android:layout_marginStart="16dp" app:layout_constraintStart_toStartOf="parent"/>

    <com.sdsmdg.harjot.crollerTest.Croller
            android:id="@+id/croller_cinco"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_centerInParent="false"
            app:back_circle_color="#EDEDED"
            app:indicator_color="#0B3C49"
            app:indicator_width="10"
            app:is_continuous="false"
            app:label_color="#000000"
            app:main_circle_color="#FFFFFF"
            app:max="20"
            app:min="0"
            app:label="Número 5s: 0"
            app:progress_primary_color="#0B3C49"
            app:progress_secondary_color="#EEEEEE"
            app:start_offset="45"
            android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/croller_siete"
    />

    <Button
            android:text="Listo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_listo_add_course_evals" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"/>


</android.support.constraint.ConstraintLayout>